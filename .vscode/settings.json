{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "tailwindCSS.validate": true,
  "tailwindCSS.emmetCompletions": true,
  "emmet.includeLanguages": {
    "javascript": "html",
    "typescript": "html"
  }
}
